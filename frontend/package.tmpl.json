    "eslint-plugin-strict-dependencies": "^1.3.3",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.2"
  },
